{
  "compilerOptions": {
    "target": "ES5",
    "module": "ESNext",
    "lib": ["ES5", "ScriptHost"],
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "importHelpers": false,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "strict": true,
    "baseUrl": ".",
    "composite": true,
    "declaration": true
  }
}
